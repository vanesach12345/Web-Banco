{
  "name": "web-banco",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:frontend": "ng serve",
    "start:backend": "npm run dev --prefix backend",
    "start:all": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "star": "ng serve"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@angular/common": "^20.2.0",
    "@angular/compiler": "^20.2.0",
    "@angular/core": "^20.2.0",
    "@angular/forms": "^20.2.0",
    "@angular/platform-browser": "^20.2.0",
    "@angular/platform-server": "^20.2.0",
    "@angular/router": "^20.2.0",
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^6.0.0",
    "express": "^5.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-html2canvas": "^1.5.2",
    "multer": "^2.0.2",
    "pdf-parse": "^2.4.5",
    "rxjs": "~7.8.0",
    "tesseract.js": "^6.0.1",
    "tslib": "^2.3.0"
